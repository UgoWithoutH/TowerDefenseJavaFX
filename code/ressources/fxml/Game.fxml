<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet menustyle.css?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<GridPane fx:id="gridPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Game">
<HBox alignment="CENTER" spacing="5" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <Button mnemonicParsing="false" text="GiveUp" onAction="#giveUp"/>
      <Button fx:id="buytower" mnemonicParsing="false" onAction="#buyTower"/>
      <Button fx:id="pauseRestart" mnemonicParsing="false" text="Stop" onAction="#pauseOrRestart"/>
      <Button fx:id="speed" mnemonicParsing="false" text="X2" onAction="#speed"/>
      <Label fx:id="textScore"/>
      <Label fx:id="coins"/>
      <StackPane fx:id="coeur"/>
      <Label fx:id="level"/>
</HBox>
</GridPane>
